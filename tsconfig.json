{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ESNext",
    "jsx": "react",
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Development",
  "include": ["src"],
  "exclude": ["node_modules"]
}
